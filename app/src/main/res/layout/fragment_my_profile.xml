<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:id="@+id/fragment_my_profile">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#212432"
            android:orientation="vertical">

            <TextView
                android:id="@+id/profile_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/oregano"
                android:text="@string/my_profile"
                android:textColor="#FFFFFF"
                android:textSize="24sp"
                android:layout_gravity="center_horizontal"/>

            <TableRow
                android:id="@+id/Row1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="16dp"
                app:layout_constraintTop_toBottomOf="@+id/profile_title">

                <TextView
                    android:id="@+id/userName"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/oregano"
                    android:text="@string/userName"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />

                <EditText
                    android:id="@+id/showName"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:ems="3"
                    android:enabled="false"
                    android:maxLength="50"
                    android:inputType="textMultiLine"
                    android:textColor="#FFFFFF"
                    android:textAlignment="textEnd"
                    tools:ignore="Autofill,LabelFor" />

            </TableRow>

            <TableRow
                android:id="@+id/Row2"
                app:layout_constraintTop_toBottomOf="@+id/Row1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/userSecondName"
                    android:layout_width="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/oregano"
                    android:text="@string/userName2"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />

                <EditText
                    android:id="@+id/showSecondName"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:textColor="#FFFFFF"
                    android:textAlignment="textEnd"
                    android:maxLength="50"
                    android:ems="5"
                    android:inputType="textMultiLine"
                    android:enabled="false"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/showName"
                    tools:ignore="Autofill,LabelFor" />

            </TableRow>

            <TableRow
                android:id="@+id/Row3"
                app:layout_constraintTop_toBottomOf="@+id/Row2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/userSurname"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/oregano"
                    android:text="@string/userSurname"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />

                <EditText
                    android:id="@+id/showSurname"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:textColor="#FFFFFF"
                    android:textAlignment="textEnd"
                    android:maxLength="50"
                    android:ems="4"
                    android:inputType="textMultiLine"
                    android:enabled="false"
                    tools:ignore="Autofill,LabelFor" />

            </TableRow>

            <TableRow
                android:id="@+id/Row4"
                app:layout_constraintTop_toBottomOf="@+id/Row3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/userDateOfBirth"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/oregano"
                    android:text="@string/userBirth"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />

                <EditText
                    android:id="@+id/showBirth"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:textColor="#FFFFFF"
                    android:textAlignment="textEnd"
                    android:maxLength="12"
                    android:ems="5"
                    android:inputType="text|date"
                    android:enabled="false"
                    tools:ignore="Autofill,LabelFor" />

            </TableRow>

            <TableRow
                android:id="@+id/Row5"
                app:layout_constraintTop_toBottomOf="@+id/Row4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/userGender"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/oregano"
                    android:text="@string/userGender"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />

                <EditText
                    android:id="@+id/showGender"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:textColor="#FFFFFF"
                    android:textAlignment="textEnd"
                    android:maxLength="1"
                    android:ems="4"
                    android:inputType="textPersonName"
                    android:enabled="false"
                    tools:ignore="Autofill,LabelFor" />

            </TableRow>


            <TableRow
                android:id="@+id/Row6"
                app:layout_constraintTop_toBottomOf="@+id/Row5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/userMail"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:fontFamily="@font/oregano"
                    android:text="@string/userMail"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp" />

                <EditText
                    android:id="@+id/showMail"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:textColor="#FFFFFF"
                    android:ems="4"
                    android:maxLength="50"
                    android:inputType="textMultiLine"
                    android:enabled="false"
                    android:textAlignment="textEnd"
                    tools:ignore="Autofill,LabelFor" />

            </TableRow>

            <TableRow
                android:id="@+id/Row7"
                app:layout_constraintTop_toBottomOf="@+id/Row6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

            <TextView
                android:id="@+id/userAccountNumber"
                android:layout_width="wrap_content"
                android:layout_weight="1"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/oregano"
                android:text="@string/userAccountNumber"
                android:textColor="#FFFFFF"
                android:textSize="24sp" />

            </TableRow>

            <TableRow
                android:id="@+id/Row8"
                app:layout_constraintTop_toBottomOf="@+id/Row7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <EditText
                    android:id="@+id/showNumber"
                    android:layout_width="wrap_content"
                    android:layout_weight="1"
                    android:layout_marginStart="16dp"
                    android:layout_marginEnd="16dp"
                    android:layout_marginTop="16dp"
                    android:textColor="#FFFFFF"
                    android:ems="10"
                    android:inputType="numberDecimal"
                    android:textAlignment="textEnd"
                    android:enabled="false"
                    tools:ignore="Autofill,LabelFor" />
            </TableRow>

        </LinearLayout>
    </ScrollView>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>